{
	"author": "Rowe Wilson Frederisk Holme",
	"license": "MIT",
	"source": "https://github.com/Frederisk/Wikitext-VSCode-Extension/blob/master/language-configuration.yaml",
	"comments": {
		"blockComment": [
			"<!--",
			"-->"
		]
	},
	"brackets": [
		[
			"{{",
			"}}"
		],
		[
			"{{{",
			"}}}"
		],
		[
			"-{",
			"}-"
		]
	],
	"autoClosingPairs": [
		{
			"open": "{",
			"close": "}"
		},
		{
			"open": "[",
			"close": "]"
		},
		{
			"open": "-{",
			"close": "}-"
		},
		{
			"open": "<!--",
			"close": "-->"
		}
	],
	"surroundingPairs": [
		{
			"open": "{",
			"close": "}"
		},
		{
			"open": "[",
			"close": "]"
		},
		{
			"open": "(",
			"close": ")"
		},
		{
			"open": "'",
			"close": "'"
		}
	]
}
